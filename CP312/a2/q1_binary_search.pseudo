def binary_search(list, left, right, target):

    if left > right Do
        return False
    
    middle = floor((left + right) / 2)

    if target == list[middle] Do
        return middle
    
    else if target < list[middle] Do
        return binary_search(list, left, middle-1, target)
    
    else Do 
        return binary_search(list, middle+1, right, target)
